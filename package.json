{
  "name": "keywarrior",
  "displayName": "keywarrior",
  "description": "성장하는 ASCII 전사와 함께 코딩 모험을 떠나세요!",
  "version": "0.1.0",
  "publisher": "keywarrior",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "keywarrior.start",
        "title": "Start keyWerrior"
      },
      {
        "command": "keywarrior.stop",
        "title": "Stop keyWerrior"
      },
      {
        "command": "keywarrior.status",
        "title": "Show keyWerrior Status"
      },
      {
        "command": "keywarrior.reset",
        "title": "Reset keyWerrior"
      },
      {
        "command": "keywarrior.attack",
        "title": "Attack Monster (keyWerrior)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@vscode/vsce": "^3.5.0",
    "typescript": "^5.8.3"
  }
}
